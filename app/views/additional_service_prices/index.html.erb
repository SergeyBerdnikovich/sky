<h1>Listing additional_service_prices</h1>

<table>
  <tr>
    <th>Additional service</th>
    <th>Vendoir if</th>
    <th>Per sq</th>
    <th>Flat</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @additional_service_prices.each do |additional_service_price| %>
  <tr>
    <td><%= additional_service_price.additional_service_id %></td>
    <td><%= additional_service_price.vendoir_if %></td>
    <td><%= additional_service_price.per_sq %></td>
    <td><%= additional_service_price.flat %></td>
    <td><%= link_to 'Show', additional_service_price %></td>
    <td><%= link_to 'Edit', edit_additional_service_price_path(additional_service_price) %></td>
    <td><%= link_to 'Destroy', additional_service_price, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

